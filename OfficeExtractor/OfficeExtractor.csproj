<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.14.0</Version>
    <Copyright>(c)2013-2020 Kees van Spelde</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>office ole word excel powerpoint rtf</PackageTags>
    <PackageReleaseNotes>- Code upgraded to .NET Standard 2.0</PackageReleaseNotes>
    <PackageIcon>extract-icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/Sicos1977/OfficeExtractor</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageProjectUrl>https://github.com/Sicos1977/OfficeExtractor</PackageProjectUrl>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>OfficeExtractor.snk</AssemblyOriginatorKeyFile>
    <AssemblyVersion>1.14.0.0</AssemblyVersion>
    <FileVersion>1.14.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DocumentFormat.OpenXml" Version="2.10.1" />
    <PackageReference Include="MsgReader" Version="3.7.20" />
    <PackageReference Include="OpenMcdf" Version="2.2.1.4" />
    <PackageReference Include="PasswordProtectedChecker" Version="1.3.6" />
    <PackageReference Include="SharpCompress" Version="0.25.0" />
    <PackageReference Include="SharpZipLib" Version="1.2.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="extract-icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
